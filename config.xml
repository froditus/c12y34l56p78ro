<?xml version='1.0' encoding='utf-8'?>
<widget id="com.cylpro.app" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://cordova.apache.org/ns/1.0" xmlns:android = "http://schemas.android.com/apk/res/android">
  <name>CYLPro</name>
  <description>Change Your Life Program</description>
  <author email="admin@cyl-pro.com" href="https://apps.cyl-pro.com">CYLPro</author>
  <preference name="phonegap-version" value="cli-6.5.0" />
  <preference name="permissions" value="none" />
  <preference name="orientation" value="portrait" />
  <preference name="target-device" value="universal" />
  <preference name="fullscreen" value="false" />
  <plugin name="cordova-plugin-inappbrowser" spec="1.3.0" />
  <plugin name="cordova-plugin-device" spec="1.1.2" />
  <plugin name="cordova-plugin-splashscreen" spec="3.2.2" />
  <icon src="icon.png" />
  <splash src="splash.png" />
  <content src="connect-src *" />
  <access origin="*" />
  <config-file platform="android" parent="/manifest" mode="merge">
    <uses-permission android:name="android.permission.INTERNET" />
  </config-file>
</widget>
